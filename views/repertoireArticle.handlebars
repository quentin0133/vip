<div id="containerArticle">
  <div id="article">
    <fieldset>
      <legend>Article</legend>
      <p>Choisissez un vip pour voir les article le concernant</p>
    </fieldset>
  </div>

<div class="flex"></div>

<div id="menuArticle">
  <div id="imageMenu">
    <img src="/images/vips.png">
  </div>
  <table>
    {{#each listeVip}}
      <tr>
        <td>
          <a href="/articles/{{this.VIP_NUMERO}}">{{this.VIP_PRENOM}} {{this.VIP_NOM}}</a>
        </td>
      </tr>
    {{/each}}
  </table>
</div>
